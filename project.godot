config_version=5

[application]

config/name="GameStudioProject"
run/main_scene="res://Menus/MenuScreenMenus/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://Assets/icon.png"

[autoload]

SceneController="*res://Menus/SceneTransitionAndLoading/scene_controller.gd"
SignalBus="*res://EnvironmentObjects/PortalTeleport/TelportationSignalBus.gd"
SettingsManager="*res://Menus/MenuScreenMenus/setting_manager.gd"
PathConstants="*res://Menus/path_constants.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=420
window/size/mode=3

[editor]

export/convert_text_resources_to_binary=false

